{
    "name": "wr2net/lm-create",
    "description": "LM Create is a complete module create for Laravel.",
    "version": "1.1.08",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "development",
        "modules",
        "operations"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Wagner Rigoli da Rosa",
            "email": "wagnerr2@gmail.com",
            "homepage": "https://wagner.fawacom.com.br",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.34",
        "symfony/console": "^7.2"
    },
    "require-dev": {
        "spatie/laravel-package-tools": "^1.16"
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\LaravelModuleCreate\\Providers\\ModulesServiceProvider.php"
            ]
        }
    },
    "scripts": {
        "lm-create": "./artisan lm-create"
    }
}
